FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         cellMDLimited Gauss linear 0.5;
    // grad(p)         Gauss linear;
    // grad(fw)         Gauss linear;
}

divSchemes
{
    default              none;
    // div(grad(p))        Gauss linear;
    div(phi,fw)         Gauss upwind;
    // div(phi,fw)         Gauss limitedLinear 0.333;

}

laplacianSchemes
{
    default         Gauss linear orthogonal;
    // default            Gauss linear limited 0.333;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
    // default            limited 0.333;
}


