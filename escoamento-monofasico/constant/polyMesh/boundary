FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    inlet
    {
        type            wall;
        inGroups        1(wall);
        nFaces          1;
        startFace       44;
    }
    outlet
    {
        type            wall;
        inGroups        1(wall);
        nFaces          1;
        startFace       45;
    }
    emptyWalls
    {
        type            empty;
        inGroups        1(empty);
        nFaces          180;
        startFace       46;
    }
)

